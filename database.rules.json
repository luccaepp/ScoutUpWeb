{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "usuario": {
      ".indexOn": ["secao/chave", "grupo", "patrulha/chave"]
    },
    "mapAtividadePH": {
      ".indexOn": ["chavePH"]
    },
    "grupo": {
      "$uidGrupo": {
        "secoes": {
          ".indexOn": ["nome"],
          "$uidSecao": {
            "posts": {
              ".indexOn": ["timeStampNeg"]
            }
          }
        }
      }
    }
  }
}